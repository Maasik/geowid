<?xml version="1.0" encoding="UTF-8"?>
<configuration status="OFF">
    <appenders>
        <File name="logFile" fileName="logs/geowidsrv.log">
            <PatternLayout>
                <pattern>%d{dd MMM yyyy HH:mm:ss.SSS} [%t] %-5level %logger{36} :: %msg%n</pattern>
            </PatternLayout>
        </File>
    </appenders>
    <loggers>
        <root level="info">
            <appender-ref ref="logFile"/>
        </root>
    </loggers>
</configuration>